<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.szip.user">

    <application
        android:name="debug.MainApplication"
        android:allowBackup="true"
        android:label="@string/user_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserSetActivity"></activity>
        <activity android:name=".NotificationActivity" />
        <activity android:name=".UnitSelectActivity" />
        <activity android:name=".search.DeviceActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
    </application>

</manifest>